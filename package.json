{
  "name": "vlex",
  "version": "0.0.3",
  "description": "Vue like Expression",
  "main": "dist/src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "cov": "jest --coverage",
    "lint": "eslint --ext .js,.ts ./ --fix",
    "prepublish": "npm run build",
    "release": "./node_modules/.bin/verpub publish",
    "release:dry": "./node_modules/.bin/verpub --dry-run"
  },
  "jest": {
    "rootDir": "./",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "coverageReporters": [
      "json",
      "lcov",
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "vue",
    "dynamic expression"
  ],
  "author": {
    "name": "Gcaufy<gcaufy@gmail.com>"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.6.4",
    "eslint": "^7.9.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3",
    "verpub": "^0.1.4",
    "vue": "^2.6.12"
  },
  "readme": "README.md",
  "verpub": {
    "publish": {
      "client": "npm"
    },
    "tag": "{name}@{version}"
  },
  "dependencies": {
    "vue-template-compiler": "^2.6.12"
  },
  "_id": "vlex@0.0.3",
  "_commitid": "f3d00c3"
}
